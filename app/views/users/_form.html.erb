<%= simple_form_for user do |form| %>
  <%= form.input :name %>
  <%= form.input :title %>
  <%= form.input :email %>
  <%= form.input :password, hint: '' %>
  <%= form.input :password_confirmation, hint: '' %>
  <div class="submit-wrapper clearfix">
    <%= form.button :submit %>
    <div class="side-right"></div>
  </div>
<% end %>

