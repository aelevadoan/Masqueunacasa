<% google_maps %>
<%= page do %>
  <%= user_column 'span2' do %>
    <div class="content-inner">
      <%= avatar_image(user) %>
      <%= render_kudos user %>
    </div>
  <% end %>

  <div class="center span6">
    <div class="content">
      <%= render_agent_info user %>
    </div>
  </div>
  <div class="sidebar sidebar-right span4">
    <%= render_locations_bar user %>
  </div>
<% end %>
